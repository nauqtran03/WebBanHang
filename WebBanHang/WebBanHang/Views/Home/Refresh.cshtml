@model WebBanHang.Models.ReportModel
@if (Model != null)
{
    <div class="report">
        <table class="table">
            <tr>
                <td>Today</td>
                <td>@Model.HomNay</td>
            </tr>
            <tr>
                <td>Yesterday</td>
                <td>@Model.HomQua</td>
            </tr>
            <tr>
                <td>The week</td>
                <td>@Model.TuanNay</td>
            </tr>
            <tr>
                <td>User online</td>
                <td>@ViewBag.Visitors_online</td>
            </tr>
        </table>
    </div>
}

